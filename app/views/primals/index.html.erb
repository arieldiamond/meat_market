<h1>Listing primals</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @primals.each do |primal| %>
      <tr>
        <td><%= primal.name %></td>
        <td><%= link_to 'Show', primal %></td>
        <td><%= link_to 'Edit', edit_primal_path(primal) %></td>
        <td><%= link_to 'Destroy', primal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Primal', new_primal_path %>
